{
    "version": 2,
    "builds": [
      {
        "src": "property/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "vercel/output"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/"
      },
      { 
        "src": "/(.*)", 
        "dest": "server.py" 
      }
    ]
  }